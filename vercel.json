{
  "builds": [
    { "src": "backend/package.json", "use": "@vercel/node" }
  ],

  "routes": [
    {
      "src": "/", "dest": "index.js"},
    {
      "src": "/api/books", "methods": ["GET"],"dest": "api/enrutador.js"},
    {
      "src": "/api/books","methods": ["POST"],"dest": "api/enrutador.js"},
      {
      "src": "/api/books","methods": ["DELETE"],"dest": "api/enrutador.js"}
  ]
}