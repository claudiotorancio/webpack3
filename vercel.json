{
  "version": 2,
 
  
  
  "routes": [
    {
      "src": "/", "dest": "backend/index.js"},
      {
        "src": "/api/books", "methods": ["GET"],"dest": "api/books.js"},
      {
        "src": "/api/books","methods": ["POST"],"dest": "api/books.js"},
        {
          "src": "/api/books","methods": ["DELETE"],"dest": "api/books.js"},
    {
      "src": "/(.*)",
      "dest": "/"
    }
   
  ]
}
