{
  "builds": [
  
    { "src": "/package.json", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/", "dest": "backend/index.js"
    },
    {
      "src": "/api/books", "methods": ["GET"],"dest": "backend/api/books.js"
    },
    {
      "src": "/api/books", "methods": ["POST"],"dest": "backend/api/books.js"
    },
    {
      "src": "/api/books", "methods": ["DELETE"],"dest": "backend/api/books.js"
    }
  ]
}
